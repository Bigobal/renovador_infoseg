{
  "manifest_version": 2,
  "name": "Renovador sessão Infoseg",
  "version": "1.1",
  "description": "Renovador sessão Infoseg.",
  "permissions": [
    "activeTab",
    "https://infoseg.sinesp.gov.br/infoseg2/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://infoseg.sinesp.gov.br/infoseg2/*"],
      "js": ["content.js"]
    }
  ]
}
